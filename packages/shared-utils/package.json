    "build": "pnpm build:core",
    "build:core": "rm -rf dist && tsdown && cp -R ./src/css ./dist",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
