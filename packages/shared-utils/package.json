{
  "name": "@coldsurfers/shared-utils",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/",
  "files": ["src", "dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
