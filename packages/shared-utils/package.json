  "peerDependencies": {
    "jwt-decode": "^4.0.0",
    "schema-dts": "^1.1.2",
    "zod": "^3.23.8"
  },
  "peerDependenciesMeta": {
    "jwt-decode": {
      "optional": true
    },
    "schema-dts": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
